/* ----------- Non-Retina Screens ----------- */
@media screen
  and (min-device-width: 1200px)
  and (max-device-width: 1300px)
  and (-webkit-min-device-pixel-ratio: 1) {
    #left-nav .nav-tabs > li {
    width: 109px!important;
}
#alert .panel-body {
    padding: 18.6px !important;
}
#task .panel-body {
    padding: 15.9px !important;
}
#notification .blue-caret {left: 85% !important;}

#userprofile .blue-caret {
    left: 93% !important;
}

#notification .dropdown-menu {left: -303px;}
#userprofile .dropdown-menu {left: -399px !important;}
}

/* ----------- Retina Screens ----------- */
@media screen
  and (min-device-width: 1200px)
  and (max-device-width: 1300px)
  and (-webkit-min-device-pixel-ratio: 2)
  and (min-resolution: 192dpi) {
  #left-nav .nav-tabs > li {
    width: 109px;
}
#alert .panel-body {
    padding: 22.2px !important;
}
#task .panel-body {
    padding: 18.4px !important;
}
.blue-caret {left: 93% !important;}
#notification .dropdown-menu {left: -303px;}
#userprofile .dropdown-menu {left: -399px !important;}
}




@media only screen and (min-device-width : 768px) and (max-device-width : 1024px) {

#left-nav .nav-tabs > li {
    width: 87px !important;
}
